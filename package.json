{
  "name": "kagi-dark-theme",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "sass --update --no-source-map --style=compressed ./src/kagi_dark.scss ./build/kagi_dark.min.css",
